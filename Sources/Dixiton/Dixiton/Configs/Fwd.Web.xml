<?xml version="1.0" encoding="utf-8"?>
<objects xmlns="http://www.springframework.net" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <object id="IContextInformer" type="Dixiton.Web.ContextInformer.WebContextInformer, Dixiton" singleton="true" autowire="autodetect"/>



  <object id="ActivityApiController" type="Dixiton.Web.Controllers.Api.ActivityApiController, Dixiton" autowire="autodetect" singleton="false" >
    <property name="CommandQueryDispatcher" ref="CommandQueryDispatcher" />
  </object>


 

  <object id="IResourceProvider" type="Dixiton.Web.ResourceProvider, Dixiton" init-method="Init" singleton="true"  >
    <property name="StringResources">
      <list>
        <value>Resources.Strings</value>
      </list>
    </property>
  </object>

 
</objects>